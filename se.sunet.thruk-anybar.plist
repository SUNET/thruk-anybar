<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>se.sunet.thruk-anybar</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/%%WHOAMI%%/.Anybar/thruk-anybar.py</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>StandardErrorPath</key>
    <string>/tmp/thruk-anybar-stderr.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/thruk-anybar-stdout.log</string>
    <key>EnvironmentVariables</key>
    <dict>
    <key>PATH</key>
    <string><![CDATA[/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin]]>
    </string>
    </dict>
    </dict>
</plist>
